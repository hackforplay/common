{
  "name": "@hackforplay/common",
  "version": "0.11.7",
  "description": "Common package for building stages in hackforplay",
  "main": "lib/index.js",
  "unpkg": "dist/main.min.js",
  "scripts": {
    "start": "webpack-dev-server",
    "prepublish": "webpack && npm run babel && npm test",
    "babel": "babel -d lib -D src --extensions '.js,.ts'",
    "format": "prettier --write '**/*.js'",
    "test": "ava",
    "watch": "ava \"test/index.js\" --watch",
    "resources": "gsutil -m -h \"Cache-Control:public, max-age=3600\" cp -r src/resources gs://hackforplay-common",
    "cors": "gsutil cors set cors.json gs://hackforplay-common"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hackforplay/common.git"
  },
  "keywords": [
    "hackforplay"
  ],
  "author": "teramotodaiki",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hackforplay/common/issues"
  },
  "homepage": "https://github.com/hackforplay/common#readme",
  "ava": {
    "compileEnhancements": false,
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register",
      "canvas-prebuilt",
      "raf/polyfill",
      "./test/helpers/setup-browser-env.js",
      "./test/helpers/setup-feeles-env.js"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-transform-runtime": "^7.1.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-typescript": "^7.1.0",
    "@babel/register": "^7.0.0",
    "ava": "1.0.0-beta.8",
    "babel-loader": "^8.0.2",
    "browser-env": "^3.2.5",
    "canvas-prebuilt": "^1.6.5-prerelease.1",
    "prettier": "^1.14.2",
    "raf": "^3.4.1",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0",
    "typescript": "^3.1.6",
    "webpack": "^4.17.2",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.9"
  },
  "dependencies": {
    "@babel/runtime": "^7.1.2"
  },
  "engines": {
    "node": "^10.13.0"
  }
}
